{
  "version": "1.0.0",
  "name": "Bigger Picture",
  "description": "Make online photo galleries more pretty.",
  "manifest_version": 2,
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.boston.com/bigpicture/*.html"
      ],
      "css": ["css/normalize.css","css/bigslides.css"],
      "js": [ "jquery.js", "js/content.js"],
      "run_at": "document_end"
    }
  ]
}
